{
  "name": "react-cypress-github-actions",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "merge-cy:reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/report.json && rm -R -f cypress/reports/mocha",
    "generate-cy:report": "marge cypress/reports/report.json -f report -i -o cypress/reports",
    "pre-test-e2e": "rm -R -f cypress/reports",
    "post-test-e2e": "npm run merge-cy:reports && npm run generate-cy:report",
    "test-e2e": "npm run pre-test-e2e && cypress run --browser chrome --config-file ./cypress/config/cypress.config.ts || npm run post-test-e2e",
    "test-e2e-gui": "npm run pre-test-e2e && cypress run --browser chrome --headed --config-file ./cypress/config/cypress.config.ts || npm run post-test-e2e",
    "test-e2e-open": "cypress open --browser chrome --config-file ./cypress/config/cypress.config.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.2",
    "http-proxy-middleware": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/mochawesome": "^6.2.4",
    "@types/node": "^16.18.104",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "cypress": "^13.13.1",
    "cypress-mochawesome-reporter": "^3.8.2",
    "dotenv": "^16.4.5",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "typescript": "^4.9.5"
  }
}
